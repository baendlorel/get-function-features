{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs", // 必须为 commonjs
    "target": "es6",
    "outDir": "dist",
    "esModuleInterop": true, // 可选，增强兼容性
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/*.ts", "src/misc/index.ts", "src/misc/logs.ts"],
  "exclude": ["tests"]
}
